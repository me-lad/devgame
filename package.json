  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0"
  },
  "dependencies": {
    "jquery": "^3.7.1"
  }
}
